@{ 
    int userid = 0;
    try
    {
        userid = int.Parse(Session["userid"].ToString());
    }
    catch (Exception ex)
    {
        Response.Redirect("~/Home/Logout");
        return;
    }
    var currentUser = new Database().tbl_users.Where(x => x.Id == userid).FirstOrDefault();
}

